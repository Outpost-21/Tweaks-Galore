			<li>Tweak_PowerAdj_SunLamp</li>
			<li>Tweak_PowerAdj_Autodoor</li>
			<li>Tweak_PowerAdj_VanometricPowerCell</li>
			<li>Tweak_PowerAdj_TubeTelevision</li>
			<li>Tweak_PowerAdj_FlatscreenTelevision</li>
			<li>Tweak_PowerAdj_MegascreenTelevision</li>
			<!-- ========== Biotech ========== -->
			<li>Tweak_PowerAdj_DeathrestCasket</li>
			<li>Tweak_PowerAdj_DeathrestBuildings</li>
			<li>Tweak_PowerAdj_DeathrestAccelerator</li>
			<li>Tweak_PowerAdj_BandNode</li>
			<li>Tweak_PowerAdj_ToxifierGenerator</li>
			<li>Tweak_PowerAdj_GeneAssembler</li>
			<li>Tweak_PowerAdj_GeneBank</li>
			<li>Tweak_PowerAdj_GeneExtractor</li>
			<li>Tweak_PowerAdj_GrowthVat</li>
			<li>Tweak_PowerAdj_PollutionPump</li>
			<li>Tweak_PowerAdj_WastepackAtomizer</li>
			<li>Tweak_PowerAdj_GeneProcessor</li>
			<li>Tweak_PowerAdj_MechBooster</li>
			<li>Tweak_PowerAdj_MechGestator</li>
			<li>Tweak_PowerAdj_LargeMechGestator</li>
			<li>Tweak_PowerAdj_SubcoreEncoder</li>
			<li>Tweak_PowerAdj_SubcoreScanners</li>
			<li>Tweak_PowerAdj_BasicRecharger</li>
			<li>Tweak_PowerAdj_StandardRecharger</li>
		</tweaks>
	</TweaksGalore.TweakSubSectionDef>

