<?xml version="1.0" encoding="utf-8"?>
<Patch>

	<Operation Class="TweaksGalore.PatchOperation_TweakBool">
		<settings>
			<li>Tweak_AnimaTweaks</li>
		</settings>
		<active Class="TweaksGalore.PatchOperation_TweakBool">
			<settings>
				<li>Tweak_ReplantableAnima</li>
			</settings>
			<active Class="PatchOperationConditional">
				<xpath>/Defs/ThingDef[defName="Plant_TreeAnima"]/minifiedDef</xpath>
				<match Class="PatchOperationRemove">
					<xpath>/Defs/ThingDef[defName="Plant_TreeAnima"]/minifiedDef</xpath>
				</match>
			</active>
		</active>
	</Operation>

</Patch>