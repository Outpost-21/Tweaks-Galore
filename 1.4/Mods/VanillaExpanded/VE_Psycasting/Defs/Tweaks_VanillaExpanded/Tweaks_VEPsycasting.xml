<?xml version="1.0" encoding="UTF-8"?>
<Defs>

	<TweaksGalore.TweakSectionDef>
		<defName>TweakSection_VanillaPsycastingExpanded</defName>
		<label>Vanilla Psycasting Expanded</label>
		<tweaks>
			<li>Tweak_HealableSacrificialFingers</li>
		</tweaks>
		<required>
			<li>VanillaExpanded.VPsycastsE</li>
		</required>
	</TweaksGalore.TweakSectionDef>

	<TweaksGalore.TweakDef>
		<defName>Tweak_HealableSacrificialFingers</defName>
		<label>Healable Sacrificial Fingers</label>
		<description>If enabled, allows methods that typically regrow body parts to regrow sacrificed fingers.</description>
		<tweakType>Bool</tweakType>
		<defaultValue>false</defaultValue>
		<required>
			<li>VanillaExpanded.VPsycastsE</li>
		</required>
	</TweaksGalore.TweakDef>

</Defs>