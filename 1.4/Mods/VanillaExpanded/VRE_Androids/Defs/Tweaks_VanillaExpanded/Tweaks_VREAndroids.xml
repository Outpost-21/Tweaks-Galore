<?xml version="1.0" encoding="UTF-8"?>
<Defs>

	<TweaksGalore.TweakSectionDef>
		<defName>TweakSection_VREAndroids</defName>
		<label>Vanilla Races Expanded - Androids</label>
		<tweaks>
			<li>Tweak_AllowAwakenedModification</li>
			<li>Tweak_DisableAwakening</li>
		</tweaks>
		<required>
			<li>vanillaracesexpanded.android</li>
		</required>
	</TweaksGalore.TweakSectionDef>

	<TweaksGalore.TweakDef>
		<defName>Tweak_AllowAwakenedModification</defName>
		<label>Allow Awakened Modification</label>
		<description>If enabled, allows awakened androids to be modified.</description>
		<tweakType>Bool</tweakType>
		<defaultValue>false</defaultValue>
		<required>
			<li>vanillaracesexpanded.android</li>
		</required>
	</TweaksGalore.TweakDef>

	<TweaksGalore.TweakDef>
		<defName>Tweak_DisableAwakening</defName>
		<label>Disable Awakening</label>
		<description>If enabled, unawakened androids from awakening.</description>
		<tweakType>Bool</tweakType>
		<defaultValue>false</defaultValue>
		<required>
			<li>vanillaracesexpanded.android</li>
		</required>
	</TweaksGalore.TweakDef>

</Defs>